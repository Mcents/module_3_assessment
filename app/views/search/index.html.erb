<h2><%= @store_total%> Total Stores</h2>

<div class="table">
<table style="width:95%">
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone</th>
    <th>Store Type</th>
    </tr>
    <tr>
    <% @stores.each do |s| %>
      <td><%= s.long_name %></td>
      <td><%= s.city %></td>
      <td><%= s.distance %></td>
      <td><%= s.phone %></td>
      <td><%= s.store_type %></td>
    </tr>
    <% end %>
</table>
</div>
